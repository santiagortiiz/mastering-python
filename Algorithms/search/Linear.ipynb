   "source": [
    "# Linear Search"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "9e009fe2",
   "metadata": {},
   "outputs": [],
   "source": [
    "# O(n)\n",
    "def linear_search(items, target):\n",
    "    match = False # O(1)\n",
    "    \n",
    "    for i in items: # O(n)\n",
    "        if i == target:\n",
    "            match = True\n",
    "            break\n",
    "    \n",
    "    return match"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "c37b9e2c",
   "metadata": {},
   "outputs": [
